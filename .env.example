# Enter your environment variables here and rename the file to ".env"

# Insert mongodb uri connection string here. e.g "mongodb://localhost/db"
MONGODB="{MONGODB_URI_CONNECTION_STRING}"

# Insert port that express is listening on here (default 3001)
PORT="{EXPRESS_PORT}"

# AUTH0 Variables
AUTH0_SECRET="{AUTH0_SECRET}"
AUTH0_CLIENT_ID="{AUTH0_CLIENT_ID}"